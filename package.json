  "license": "ISC",
  "devDependencies": {
    "vite": "^6.1.1",
    "shx": "^0.3.3",
    "chokidar": "^4.0.3",
    "marked": "^15.0.11"
  },
  "dependencies": {},
  "overrides": {
    "glob": "^11.0.1",
    "vite": {
      "esbuild": "^0.25.0"
    }
  }
}